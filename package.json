{
  "name": "nextjs-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "yarn next dev --port 3000",
    "analyze": "ANALYZE=true NODE_ENV=production yarn next build",
    "start": "next build && next start --port 4003",
    "test": "yarn jest",
    "storybook": "start-storybook --config-dir .storybook -p 6006 --ci --static-dir ./public",
    "build-storybook": "build-storybook --output-dir .storybook/builds --static-dir ./public",
    "type-check": "yarn tsc",
    "lint": "yarn next lint --fix --cache",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "next": "11.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@next/bundle-analyzer": "11.1.0",
    "@storybook/addon-a11y": "6.3.7",
    "@storybook/addon-actions": "6.3.7",
    "@storybook/addon-console": "1.2.3",
    "@storybook/addon-essentials": "6.3.7",
    "@storybook/addon-knobs": "6.3.0",
    "@storybook/addon-links": "6.3.7",
    "@storybook/addon-queryparams": "6.2.9",
    "@storybook/addon-storysource": "6.3.7",
    "@storybook/addon-viewport": "6.3.7",
    "@storybook/addons": "6.3.7",
    "@storybook/builder-webpack5": "6.3.7",
    "@storybook/manager-webpack5": "6.3.7",
    "@storybook/react": "6.3.7",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^27.0.1",
    "@types/node": "16.7.1",
    "@types/react": "17.0.19",
    "@types/webpack-env": "1.16.2",
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "dotenv": "^10.0.0",
    "esbuild": "^0.12.22",
    "esbuild-jest": "^0.5.0",
    "esbuild-register": "^3.0.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-next": "^11.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "jest": "^27.0.6",
    "msw": "^0.35.0",
    "next-compose-plugins": "2.2.1",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "2.3.2",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^27.0.5",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "4.3.5",
    "vercel": "23.1.2",
    "whatwg-fetch": "^3.6.2"
  },
  "resolutions": {
    "eslint-plugin-import": "^2.24.1"
  },
  "prettier": {
    "arrowParens": "always",
    "printWidth": 110,
    "semi": true,
    "trailingComma": "es5"
  },
  "browserslist": {
    "production": [
      ">0.1%",
      "not dead",
      "not op_mini all"
    ],
    "staging": [
      ">0.1%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version"
    ]
  }
}
